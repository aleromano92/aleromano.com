        {
            "type": "timeseries",
            "title": "Target Uptime (% over last 6h)",
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "100 * avg_over_time(up[6h])",
                    "legendFormat": "{{job}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100
                },
                "overrides": []
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 5
