        },
        {
            "type": "timeseries",
            "title": "Scrape Duration (95th percentile)",
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "quantile by (job) (0.95, scrape_duration_seconds)",
                    "legendFormat": "{{job}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
