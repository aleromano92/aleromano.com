{
    "id": null,
    "uid": "endpoint-health",
    "title": "Endpoint Health",
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 2,
    "refresh": "30s",
    "panels": [
        {
            "type": "stat",
            "title": "Targets Up",
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "sum(up)",
                    "refId": "A"
                }
            ],
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "orientation": "auto",
                "textMode": "value"
            },
            "fieldConfig": {
                "defaults": {
                    "decimals": 0,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "green",
                                "value": 1
                            }
                        ]
                    }
                },
                "overrides": []
            },
            "gridPos": {
                "h": 5,
                "w": 6,
                "x": 0,
                "y": 0
            }
        },
        {
            "type": "stat",
            "title": "Total Targets",
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "count(up)",
                    "refId": "A"
                }
            ],
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "orientation": "auto",
                "textMode": "value"
            },
            "fieldConfig": {
                "defaults": {
                    "decimals": 0
                },
                "overrides": []
            },
            "gridPos": {
                "h": 5,
                "w": 6,
                "x": 6,
                "y": 0
            }
        },
        {
            "type": "stat",
            "title": "Prometheus",
            "datasource": "Prometheus",
            "targets": [
                {
                    "expr": "up{job=\"prometheus\"}",
                    "refId": "A"
                }
            ],
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "orientation": "auto",
                "textMode": "value_and_name"
            },
            "fieldConfig": {
                "defaults": {
                    "mappings": [
                        {
